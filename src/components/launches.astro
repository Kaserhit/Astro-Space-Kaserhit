---
import CardLaunch from './cardLaunch.astro'
import { getLastestLaunches } from '../services/spaceX';
const { order } = Astro.props;

const launches = await getLastestLaunches(order)

---

<div class = "grid gap-8 sm:grid-cols-2 lg:grid-cols-3">
    {launches.map(launch => (
    <CardLaunch
    id={launch.id}
    img={launch.links.patch.small}
    sucess={ launch.success}
    flightNumber={launch.flight_number}
    details={launch.details}
    />
    ))}
  
</div>
